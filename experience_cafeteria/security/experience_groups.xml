<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="module_category_experience" model="ir.module.category">
            <field name="name">Experiencia Cliente</field>
            <field name="sequence">20</field>
            <field name="visible" eval="True"/>
        </record>

        <record id="privilege_experience" model="res.groups.privilege">
            <field name="name">Gesti√≥n de Experiencia</field>
            <field name="category_id" ref="module_category_experience"/>
        </record>

        <record id="group_experience_user" model="res.groups">
            <field name="name">Usuario</field>
            <field name="privilege_id" ref="privilege_experience"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_experience_manager" model="res.groups">
            <field name="name">Responsable</field>
            <field name="privilege_id" ref="privilege_experience"/>
            <field name="implied_ids" eval="[(4, ref('group_experience_user'))]"/>
        </record>
    </data>
</odoo>